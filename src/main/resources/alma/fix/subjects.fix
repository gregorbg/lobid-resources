    add_field("subject[].$last.source.label","Gemeinsame Normdatei (GND)")
    add_field("subject[].$last.source.id","https://d-nb.info/gnd/7749153-1")
    #      <data name="label" source="$[field]??.a" />
    copy_field("$i.a","subject[].$last.label")

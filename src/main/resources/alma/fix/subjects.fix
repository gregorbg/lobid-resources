do list(path:"610??|650??", "var":"$i")
  if any_match("$i.0","^\\(DE-588\\)(.*)$")
        # TODO: Deduplicate values.
        #          <data source="$[field]?4.a" name="label">
        #            <unique/>
        #          </data>
    set_array("subject[].$append.type[]","Concept")
  #        <data source="$[field]?0.a" name="label">
